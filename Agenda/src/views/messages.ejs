<% if(messages.length > 0 ) { %>
    <div class="row">
        <div class="col my-3">

            <% if(error[0] === 'true') { %>
                <div class="alert alert-danger">
            <% } else { %>
                <div class="alert alert-primary">    
            <% } %>        

            <% messages.forEach( message => { %>
                <%= message %><br>
                
            <% }); %>
            </div>
        </div>
    </div>
<% } %>


